<?xml version="1.0" encoding="utf-8"?>

<!--
	HF_Delivery scene config.
	This is when NPC gives birth
-->

<ScenesConfig xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<Scenes>
		<Scene id="HF_Delivery">
			<Performers>
				<Performer id="HF_FemaleNative_Delivery">
					<StartConditions>
						<Condition xsi:type="PregnantCheck" actor="FemaleNative#15" value="true" />
					</StartConditions>
				</Performer>
				<Performer id="HF_NativeGirl_Delivery">
					<StartConditions>
						<Condition xsi:type="PregnantCheck" actor="NativeGirl#16" value="true" />
					</StartConditions>
				</Performer>
				<Performer id="HF_FemaleLargeNative_Delivery">
					<StartConditions>
						<Condition xsi:type="PregnantCheck" actor="FemaleLargeNative#17" value="true" />
					</StartConditions>
				</Performer>
				<Performer id="HF_UnderGroundWoman_Delivery">
					<StartConditions>
						<Condition xsi:type="PregnantCheck" actor="UnderGroundWoman#44" value="true" />
					</StartConditions>
				</Performer>

				<!-- new in v0.5 -->
				<Performer id="HF_Yona_Delivery">
					<StartConditions>
						<Condition xsi:type="PregnantCheck" actor="Yona#0" value="true" />
					</StartConditions>
				</Performer>
				<Performer id="HF_YoungLady_Delivery">
					<StartConditions>
						<Condition xsi:type="PregnantCheck" actor="YoungLady#9" value="true" />
					</StartConditions>
				</Performer>
			</Performers>
		</Scene>
	</Scenes>
</ScenesConfig>
