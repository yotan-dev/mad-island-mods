<?xml version="1.0" encoding="utf-8"?>

<!-- 
	HF_ManRapesSleep scene config.
	This is when Man player grapples a sleeping NPC
-->

<ScenesConfig xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<Scenes>
		<Scene id="HF_ManRapesSleep">
			<Performers>
				<Performer id="HF_Man_FemaleNative_SleepRape_Normal">
					<StartConditions>
						<Condition xsi:type="PregnantCheck" actor="FemaleNative#15" value="false" />
					</StartConditions>
				</Performer>
				<Performer id="HF_Man_NativeGirl_SleepRape_Normal">
					<StartConditions>
						<Condition xsi:type="PregnantCheck" actor="NativeGirl#16" value="false" />
					</StartConditions>
				</Performer>
			</Performers>
		</Scene>
	</Scenes>
</ScenesConfig>
